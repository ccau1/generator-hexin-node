{
  "name": "<%= name %>",
  "description": "<%= name %>",
  "version": "0.1.0",
  "license": "MIT",
  "keywords": [
    "api",
    "logistics",
    "wtt"
  ],
  "author": "<%= name %> <support@<%= name %>.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ccau1/hexin.git"
  },
  "main": "server.js",
  "scripts": {
    "apidoc": "apidoc -i app/controllers/ -o public/docs/",
    "build": "npm run apidoc",
    "g:controller": "plop Controller",
    "g:model": "plop Model",
    "g:service": "plop Service",
    "g:msc": "plop MSC",
    "g:app_start": "plop App_Start",
    "i": "yarn install --frozen-lockfile",
    "format":
      "prettier --write \"{app,app_start,test,locales,configs}/**/*.js\"",
    "nvm":
      "~/.nvm/nvm-exec which node | xargs -J % ln -sf % ./.nvm-node && chmod +x ./.nvm-node",
    "plop": "plop",
    "seed": "node seeder",
    "seed:remove": "node seederRemove",
    "start": "node server",
    "test": "mocha --config __test__/mocha.opts ./__test__/**/*.js",
    "test:coverage": "nyc npm test",
    "test:watch": "npm test -- --watch",
    "lint": "eslint --fix {app,app_start,locales,configs}/**/*.{js,jsx}",
    "precommit": "npm run lint && npm run test:coverage",
    "eslint-check":
      "eslint --print-config .eslintrc.json | eslint-config-prettier-check"
  },
  "dependencies": {
    "@httpeace_deploy/httpeace-node-core": "^0.1.20",
    <% if (mediaPlugin) {%>"@httpeace_deploy/httpeace-plugin-media-node": "^0.1.37",<%} %>
    <% if (pagePlugin) {%>"@httpeace_deploy/httpeace-plugin-page-node": "^0.1.4",<%} %>
    <% if (postPlugin) {%>"@httpeace_deploy/httpeace-plugin-post-node": "^0.1.16",<%} %>
    <% if (postPlugin) {%>"@httpeace_deploy/httpeace-plugin-post-constants": "^0.1.4",<%} %>
    "app-module-path": "^2.2.0",
    "bcrypt-nodejs": "0.0.3",
    "express": "^4.16.2",
    "express-handlebars": "^3.0.0",
    "indicative": "2.2.1",
    "jwt-simple": "0.5.1",
    "lodash": "^4.17.4",
    "moment": "^2.19.2",
    <% if (db === 'mysql') {%>"sequelize": "^4.2.0",<%}%>
    <% if (db === 'mongo') {%>"mongodb": "^2.2.33",<%}%>
    "mongoose": "4.10.8",
    "mongoose-seed": "^0.3.2",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "sequelize": "^4.22.11"
  },
  "devDependencies": {
    "apidoc": "^0.17.5",
    "async-request": "^1.2.0",
    "chai": "^4.1.2",
    "eslint": "^4.11.0",
    "eslint-config-aftership": "^4.0.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.8.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.5.1",
    "husky": "^0.14.3",
    "mocha": "^4.0.1",
    "mock-require": "^2.0.2",
    "mongoose-mock": "^0.4.0",
    "nyc": "^11.2.1",
    "prettier": "1.8.2",
    "sinon": "^4.1.1"
  },
  "apidoc": {
    "name": "<%= name %>-API",
    "version": "0.1.0",
    "description": "<%= name %>'s API",
    "apidoc": {
      "title": "Custom apiDoc browser title",
      "url": "https://api.github.com/v1"
    }
  }
}
